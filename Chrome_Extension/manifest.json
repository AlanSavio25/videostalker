{
    "name": "Video_stalker",
    "version": "1.0",
    "description": "",
    "manifest_version": 2,
    "background": {
        "scripts": ["background.js"]
      },
    "browser_action": {        
        "default_icon":"analytics.png",
        "default_popup":"popup.html"
      },
    "permissions": ["storage",
      "activeTab",
      "tabs",
      "http://127.0.0.1:5000/*"],

    "content_scripts": [
        {
          "matches": [
            "https://www.youtube.com/w*"
          ],
          "js": ["jquery-3.4.1.min.js", "content.js","popup.js"]
        }
      ]
}

