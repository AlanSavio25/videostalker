{
    "name": "Video_stalker",
    "version": "1.0",
    "description": "",
    "manifest_version": 2,
    "background": {
        "scripts": ["background.js"],
        "persistent": false
      },
    "browser_action": {        
        "default_popup": "popup.html"
      },
    "permissions": ["storage"],
    "content_scripts": [
        {
          "matches": [
            "<all_urls>"
          ],
          "js": ["jquery-3.4.1.min.js", "content.js"]
        }
      ]
}

